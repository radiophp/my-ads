{
  "name": "my-ads-server",
  "version": "0.1.0",
  "description": "Modern NestJS backend with Fastify, Prisma, Redis, BullMQ, and Prometheus metrics.",
  "main": "dist/main.js",
  "license": "MIT",
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "dotenv -e ../.env -- nest start --watch",
    "start:debug": "dotenv -e ../.env -- nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "cron:scheduler": "ENABLE_CRON_JOBS=true dotenv -e ../.env -- ts-node -r tsconfig-paths/register ./src/scripts/run-cron-scheduler.ts",
    "build": "nest build",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "cross-env ESLINT_USE_FLAT_CONFIG=false eslint \"{src,tests}/**/*.ts\"",
    "test": "dotenv --quiet -e ../.env -- jest",
    "test:watch": "dotenv --quiet -e ../.env -- jest --watch",
    "test:cov": "dotenv --quiet -e ../.env -- jest --coverage",
    "test:e2e": "dotenv --quiet -e ../.env -- jest --config ./src/tests/e2e/jest-e2e.json",
    "prisma:generate": "dotenv -e ../.env -- prisma generate",
    "prisma:migrate": "dotenv -e ../.env -- prisma migrate dev",
    "prisma:studio": "dotenv -e ../.env -- prisma studio",
    "sync:divar-categories": "dotenv -e ../.env -- ts-node -r tsconfig-paths/register ./src/scripts/sync-divar-categories.ts",
    "sync:divar-category-filters": "dotenv -e ../.env -- ts-node -r tsconfig-paths/register ./src/scripts/sync-divar-category-filters.ts",
    "divar:harvest-posts": "dotenv -e ../.env -- ts-node -r tsconfig-paths/register ./src/scripts/run-divar-post-harvest.ts",
    "divar:fetch-posts": "dotenv -e ../.env -- ts-node -r tsconfig-paths/register ./src/scripts/run-divar-post-fetch.ts",
    "divar:analyze-posts": "dotenv -e ../.env -- ts-node -r tsconfig-paths/register ./src/scripts/run-divar-post-analyze.ts",
    "divar:sync-media": "dotenv -e ../.env -- ts-node -r tsconfig-paths/register ./src/scripts/run-divar-post-media-sync.ts",
    "divar:fetch-contacts": "dotenv -e ../.env -- ts-node -r tsconfig-paths/register ./src/scripts/run-divar-contact-fetch.ts",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.922.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/multipart": "^9.3.0",
    "@fastify/static": "^8.3.0",
    "@fastify/swagger": "^9.5.2",
    "@fastify/swagger-ui": "^2.1.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.8",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.8",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-fastify": "^11.1.8",
    "@nestjs/platform-socket.io": "^11.1.8",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/websockets": "^11.1.8",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.207.0",
    "@opentelemetry/instrumentation": "^0.207.0",
    "@opentelemetry/instrumentation-fastify": "^0.52.0",
    "@opentelemetry/instrumentation-http": "^0.207.0",
    "@opentelemetry/instrumentation-redis": "^0.56.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.207.0",
    "@prisma/client": "^6.18.0",
    "@prisma/instrumentation": "^6.18.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@types/web-push": "^3.6.4",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "amqp-connection-manager": "^5.0.0",
    "amqplib": "^0.10.9",
    "archiver": "^7.0.1",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "cache-manager": "^7.2.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie": "^1.0.2",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.4.1",
    "jalaali-js": "^1.2.8",
    "keyv": "^5.5.3",
    "nestjs-pino": "^4.4.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino": "^9.14.0",
    "pino-pretty": "^13.1.2",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.1.0",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "uuid": "^13.0.0",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.8",
    "@types/amqplib": "^0.10.7",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.9.2",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "cross-env": "^10.1.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "prisma": "^6.18.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "cross-env ESLINT_USE_FLAT_CONFIG=false eslint --max-warnings=0 --fix"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
